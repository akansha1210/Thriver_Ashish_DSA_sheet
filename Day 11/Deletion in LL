Node deleteHead(Node head)
    {
        // Your code here   
        if(head==null) return null;
        else return head.next;
    }
 static Node deleteTail(Node head)
    {
        Node cur=head;
        if(head==null) return null;
        if(head.next==null) return null;
        while(cur.next.next!=null){
            cur=cur.next;
        }
        cur.next=null;
        return head;
        // Your code here
    }
   
